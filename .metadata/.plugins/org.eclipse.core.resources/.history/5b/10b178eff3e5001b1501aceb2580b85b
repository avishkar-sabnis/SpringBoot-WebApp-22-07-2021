import java.util.Scanner;

public class ArrayMax {
		public void MaxElement() {
			int a[] = new int[10];
			Scanner sc =  new Scanner(System.in);
			System.out.println("Enter elements in array");
			for(int i=0;i<5;i++) {
				for(int j=1;j<=5;j++)
				if(a[i]>a[j]) {
					a[i]=a[j];
					i++;
				}
				else
					i++;
			}
			System.out.println(a[5]);
		}
}
